#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/sshpk/bin/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/sshpk/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/node_modules:/home/dev/Music/NewFolder/node_modules:/home/dev/Music/node_modules:/home/dev/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/sshpk/bin/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/sshpk/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/node_modules:/home/dev/Music/NewFolder/node_modules:/home/dev/Music/node_modules:/home/dev/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sshpk/bin/sshpk-verify" "$@"
else
  exec node  "$basedir/../sshpk/bin/sshpk-verify" "$@"
fi
