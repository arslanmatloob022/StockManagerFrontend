#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/stylelint@16.9.0_typescript@5.4.5/node_modules/stylelint/bin/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/stylelint@16.9.0_typescript@5.4.5/node_modules/stylelint/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/stylelint@16.9.0_typescript@5.4.5/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/node_modules:/home/dev/Music/NewFolder/node_modules:/home/dev/Music/node_modules:/home/dev/node_modules:/home/node_modules:/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/stylelint/bin/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/stylelint/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/stylelint@16.9.0_typescript@5.4.5/node_modules/stylelint/bin/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/stylelint@16.9.0_typescript@5.4.5/node_modules/stylelint/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/stylelint@16.9.0_typescript@5.4.5/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/node_modules:/home/dev/Music/NewFolder/node_modules:/home/dev/Music/node_modules:/home/dev/node_modules:/home/node_modules:/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/stylelint/bin/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/stylelint/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../stylelint/bin/stylelint.mjs" "$@"
else
  exec node  "$basedir/../stylelint/bin/stylelint.mjs" "$@"
fi
