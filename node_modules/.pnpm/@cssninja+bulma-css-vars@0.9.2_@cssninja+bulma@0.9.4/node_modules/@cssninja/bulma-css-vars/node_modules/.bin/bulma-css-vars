#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/@cssninja+bulma-css-vars@0.9.2_@cssninja+bulma@0.9.4/node_modules/@cssninja/bulma-css-vars/bin/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/@cssninja+bulma-css-vars@0.9.2_@cssninja+bulma@0.9.4/node_modules/@cssninja/bulma-css-vars/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/@cssninja+bulma-css-vars@0.9.2_@cssninja+bulma@0.9.4/node_modules/@cssninja/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/@cssninja+bulma-css-vars@0.9.2_@cssninja+bulma@0.9.4/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/node_modules:/home/dev/Music/NewFolder/node_modules:/home/dev/Music/node_modules:/home/dev/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/@cssninja+bulma-css-vars@0.9.2_@cssninja+bulma@0.9.4/node_modules/@cssninja/bulma-css-vars/bin/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/@cssninja+bulma-css-vars@0.9.2_@cssninja+bulma@0.9.4/node_modules/@cssninja/bulma-css-vars/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/@cssninja+bulma-css-vars@0.9.2_@cssninja+bulma@0.9.4/node_modules/@cssninja/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/@cssninja+bulma-css-vars@0.9.2_@cssninja+bulma@0.9.4/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/node_modules:/home/dev/Music/NewFolder/node_modules:/home/dev/Music/node_modules:/home/dev/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/bulma-css-vars.cjs" "$@"
else
  exec node  "$basedir/../../bin/bulma-css-vars.cjs" "$@"
fi
