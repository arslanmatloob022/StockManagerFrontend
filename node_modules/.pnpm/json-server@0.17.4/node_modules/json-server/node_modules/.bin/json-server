#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/json-server@0.17.4/node_modules/json-server/lib/cli/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/json-server@0.17.4/node_modules/json-server/lib/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/json-server@0.17.4/node_modules/json-server/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/json-server@0.17.4/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/node_modules:/home/dev/Music/NewFolder/node_modules:/home/dev/Music/node_modules:/home/dev/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/json-server@0.17.4/node_modules/json-server/lib/cli/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/json-server@0.17.4/node_modules/json-server/lib/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/json-server@0.17.4/node_modules/json-server/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/json-server@0.17.4/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules/.pnpm/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/StockManagerFrontend/node_modules:/home/dev/Music/NewFolder/MyWorkSpace/node_modules:/home/dev/Music/NewFolder/node_modules:/home/dev/Music/node_modules:/home/dev/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli/bin.js" "$@"
else
  exec node  "$basedir/../../lib/cli/bin.js" "$@"
fi
